{
  "name": "taist-expo",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "adb reverse tcp:8081 tcp:8081 2>/dev/null; APP_ENV=staging expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "APP_ENV=staging expo run:android",
    "ios": "APP_ENV=staging expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "dev:local": "APP_ENV=local npm start",
    "android:local": "APP_ENV=local npm run android",
    "ios:local": "APP_ENV=local npm run ios",
    "dev:staging": "APP_ENV=staging npm start",
    "android:staging": "APP_ENV=staging npm run android",
    "ios:staging": "APP_ENV=staging npm run ios",
    "dev:prod": "APP_ENV=production npm start",
    "android:prod": "APP_ENV=production npm run android",
    "ios:prod": "APP_ENV=production npm run ios",
    "storybook": "STORYBOOK_ENABLED=true expo start",
    "storybook:ios": "STORYBOOK_ENABLED=true expo run:ios",
    "storybook:android": "STORYBOOK_ENABLED=true expo run:android",
    "dev:chef-menu": "adb reverse tcp:8081 tcp:8081 2>/dev/null; DEV_SCREEN=\"/screens/chef/(tabs)/menu\" DEV_USER_TYPE=chef expo start",
