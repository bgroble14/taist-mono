{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "development"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "env": {
        "APP_ENV": "staging"
      },
      "ios": {
        "resourceClass": "m-medium",
        "submitOnBuild": true
      }
    },
    "production": {
      "env": {
        "APP_ENV": "production"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "adhoc": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {}
  },
  "cli": {
    "version": ">= 12.0.0"
  }
}