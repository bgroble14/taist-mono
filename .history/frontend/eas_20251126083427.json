{
  "cli": {
    "appVersionSource": "local",
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "development"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "env": {
        "APP_ENV": "staging"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk",
        "resourceClass": "medium"
      }
    },
    "production": {
      "env": {
        "APP_ENV": "production"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "app-bundle",
        "resourceClass": "medium"
      }
    },
    "adhoc": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "preview": {
      "ios": {
        "appleId": "billygroble@gmail.com",
        "ascAppId": "6447754932",
        "appleTeamId": "WXY2PMFQB7"
      }
    },
    "production": {
      "ios": {
        "appleId": "billygroble@gmail.com",
        "ascAppId": "6447754932",
        "appleTeamId": "WXY2PMFQB7"
      }
    }
  }
}